{
  "name": "chrome-download-directory-manager",
  "version": "1.0",
  "homepage": "https://github.com/Obstc0rp/chrome-download-directory-manager",
  "authors": [
    "Obstc0rp <wolf.scholle@gmail.com>"
  ],
