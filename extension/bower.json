    "manager",
    "directory"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "assets/bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jQuery": "2.1.1",
    "bootstrap": "3.2.0"
  }
}
